<div class="container" style="height: 14rem;">
 <h1>ジャンル編集</h1>
    <%= form_with model: [:admin,@genre], local: true do |f| %>
      <div>ジャンル名</div>
    <div class="row">
      <div class="col-md-3"><%= f.text_field :name, class: "form-control" %></div>
      <div class="col-md-2"><%= f.submit "変更を保存", class: "btn btn-outline-secondary" %></div>
    </div>
<% end %>
</div>
